  "medium": "device_config put game_overlay com.dts.freefiremax mode=2,downscaleFactor=2.5:mode=3,downscaleFactor=0.8",
  "ultra": "device_config put game_overlay com.dts.freefiremax mode=2,downscaleFactor=3.8:mode=3,downscaleFactor=0.8",
  "extreme": "device_config put game_overlay com.dts.freefiremax mode=2,downscaleFactor=7.0:mode=3,downscaleFactor=0.8"
}
